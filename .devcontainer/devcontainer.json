{
  "name": "Vibestack DevContainer",
  "build": {
    "dockerfile": "../Dockerfile",
    "context": ".."
  },
  "forwardPorts": [80],
  "portsAttributes": {
    "80": {
      "label": "Web Server",
      "onAutoForward": "openBrowser",
      "protocol": "http",
      "visibility": "public",
      "elevateIfNeeded": true
    }
  },
  "overrideCommand": false,
  "init": true,
  "postStartCommand": "echo 'Container ready!'",
  "remoteUser": "vibe"
}
