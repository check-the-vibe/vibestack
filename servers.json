{
  "mcpServers": {
    "memory": {
      "enabled": true,
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ],
      "transportType": "stdio",
      "env": {}
    },
    "sequential-thinking": {
      "enabled": true,
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ],
      "transportType": "stdio",
      "env": {}
    },
    "filesystem": {
      "enabled": true,
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "/home/vibe"
      ],
      "transportType": "stdio",
      "env": {}
    },
    "playwright": {
      "enabled": true,
      "command": "npx",
      "args": [
        "@playwright/mcp@latest",
        "--vision",
        "--isolated",
        "--no-sandbox"
      ],
      "transportType": "stdio",
      "env": {}
    }
  }
}
